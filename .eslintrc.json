{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": "warn",
    "no-alert": "off",
    "no-undef": "off",
    "no-underscore-dangle":"off",
    "no-use-before-define": "off",
    "no-shadow":"off",
    "no-console": "off",
    "no-prototype-builtins":"off"
  }
}
